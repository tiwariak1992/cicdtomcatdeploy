pipeline
{
    agent any
    stages
    {
      stage('deploy to tomcat')
          { 
            steps
            {
             sshagent(['cicd'])
              {
                sh 'scp -o StrictHostKeyChecking=no */target/*.war ec2-user@3.0.52.153:/usr/share/tomcat/webappp'
              }
           }
         }
    }
}
